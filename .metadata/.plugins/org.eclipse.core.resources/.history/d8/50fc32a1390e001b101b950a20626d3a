
public class p2CristianChaparro {

	public static void main(String[] args) {
//		Mostrar el siguiente resultado usando solo la orden println:
//
//			----- EJERCICIO DE VARIABLES Y TIPOS DE DATOS -----
//			----------------------------------------------------------------------
//			El valor de la variable casado es true
//			El valor de la variable MAXIMO es 999999
//			El valor de la variable diasem es 1
//			El valor de la variable diaanual es 300
//			El valor de la variable miliseg es 1298332800000
//			El valor de la variable totalfactura es 10350.678
//			El valor de la variable poblacion es 6775235741
//			El valor de la variable sexo es M
		
		boolean casado = true;
		int diasem = 1, diaanual = 300;
		Long miliseg = 1298332800000L, poblacion = 6775235741L;
		final int MAXIMO = 999999;
		char sexo = 'M';
		double totalfactura = 10350.678;

		System.out.println("Mostrando el contenido de la variable booleana \"casado\": " + casado);
		System.out.println("Mostrando el contenido de la constante \"MAXIMO\": " + MAXIMO);
		System.out.println("Mostrando el contenido de la variable de tipo int \"diasem\": " + diasem);
		System.out.println("Mostrando el contenido de la variable de tipo int \"diaanual\": " + diaanual);
		System.out.println("Mostrando el contenido de la variable de tipo Long \"miliseg\": " + miliseg);
		System.out.println("Mostrando el contenido de la variable de tipo double \"totalfactura\": " + totalfactura);
		System.out.println("Mostrando el contenido de la variable de tipo Long \"poblacion\": " + poblacion);
		System.out.println("Mostrando el contenido de la variable de tipo char \"sexo\": " + sexo);
		
	}

}
