
public class ExpresionesLogicas {
	
	public static void main(String[] args) {
		boolean resultado = false;
		int num1 = 45, num2 = 23;
		resultado = (num1 > 0) && (num2 > 0);
		System.out.println(resultado);
		
		char c1 = 'a', c2 = 'b', c3 = 'P';
		resultado = (c1 < c2);
		System.out.println(resultado);
				
	}
}
